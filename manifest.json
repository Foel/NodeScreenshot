{

    "manifest_version": 2,
    "name": "NodeScreenshot",
    "version": "1.0",
  
    "description": "Adds a red border to all webpages matching mozilla.org.",
  
    "icons": {
      "48": "icons/local_see-24px.svg"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.mozilla.org/*"],
        "js": ["NodeScreenshot.js"]
      }
    ]
  
  }